<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:codes="43" android:keyLabel="+" android:keyEdgeFlags="left"/>
        <Key android:codes="45" android:keyLabel="-"/>
        <Key android:codes="42" android:keyLabel="*"/>
        <Key android:codes="37" android:keyLabel="%"/>
        <Key android:codes="124" android:keyLabel="|"/>
        <Key android:codes="94" android:keyLabel="^"/>
        <Key android:codes="95" android:keyLabel="_"/>
        <Key android:codes="40" android:keyLabel="("/>
        <Key android:codes="41" android:keyLabel=")" />
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete"
                android:keyEdgeFlags="right"
                android:isRepeatable="true"/>
    </Row>

    <Row>
        <Key android:codes="126" android:keyLabel="~" android:keyEdgeFlags="left"/>
        <Key android:codes="96" android:keyLabel="`"/>
        <Key android:codes="33" android:keyLabel="!"/>
        <Key android:codes="35" android:keyLabel="\#"/>
        <Key android:codes="36" android:keyLabel="$"/>
        <Key android:codes="34" android:keyLabel="&quot;" />
        <Key android:codes="61" android:keyLabel="="/>
        <Key android:codes="91" android:keyLabel="["/>
        <Key android:codes="93" android:keyLabel="]" />
        <Key android:codes="39" android:keyLabel="\'" android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="-1002" android:keyLabel="..." android:keyEdgeFlags="left"/>
        <Key android:codes="60" android:keyLabel="&lt;"/>
        <Key android:codes="62" android:keyLabel="&gt;"/>
    
        <Key android:codes="59" android:keyLabel=";"/>
        <Key android:codes="63" android:keyLabel="\?"/>
        <Key android:codes="44" android:keyLabel="," />

        <Key android:codes="64" android:keyLabel="\@"/>
        <Key android:codes="38" android:keyLabel="&amp;"  />


        <Key android:codes="123" android:keyLabel="{"/>
        <Key android:codes="125" android:keyLabel="}" android:keyEdgeFlags="right" />
    </Row>

    <Row android:rowEdgeFlags="bottom" >
        <Key android:codes="-1004" android:keyWidth="13%p" android:keyLabel="abc"  android:keyEdgeFlags="left"/>
        <Key android:codes="-1003" android:keyWidth="12%p" android:keyLabel="123"/>
        <Key android:codes="47" android:keyWidth="10%p" android:keyLabel="/" />
        <Key android:codes="92" android:keyWidth="10%p" android:keyLabel="\\"/>
        <Key android:codes="32" android:keyWidth="20%p" android:keyIcon="@drawable/sym_keyboard_space"  android:isRepeatable="true"/>
        <Key android:codes="46" android:keyWidth="10%p" android:keyLabel="."/>
        <Key android:codes="58" android:keyWidth="10%p" android:keyLabel=":"/>
        <Key android:codes="10" android:keyWidth="15%p" android:keyIcon="@drawable/sym_keyboard_return" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>